	<!-- Login Modal -->
		<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="loginModalCenterTitle" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered" role="document">
			  <div class="modal-content">
				<div class="modal-header">
				  <h5 class="modal-title" id="loginModalCenterTitle">Login</h5>
				  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				  </button>
				</div>
				<div class="modal-body">
                    <form>
                        <div class="form-group">
                            <label for="exampleInputUsername1">Username:</label>
                            <input type="text" class="form-control" id="exampleInputUsername1" aria-describedby="emailHelp" placeholder="Enter username">
                          </div>
                          <div class="form-group">
                            <label for="exampleInputPassword1">Password:</label>
                            <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
                          </div> 
                    </form>
				</div>
				<div class="modal-footer">
				  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
				  <button type="button" class="btn btn-primary">Save changes</button>
                  <p>
                    Not a user yet? <a href="../signup.html">Signup</a>
                  </p>

				</div>
			  </div>
			</div>
		  </div>

        <!-- New Spring Modal -->
		<div class="modal fade" id="newSpringModal" tabindex="-1" role="dialog" aria-labelledby="newSpringModalCenterTitle" aria-hidden="true">
			<div class="modal-dialog modal-dialog-centered" role="document">
			  <div class="modal-content">
				<div class="modal-header">
				  <h5 class="modal-title" id="newSpringModalCenterTitle">Submit a new spring</h5>
				  <button type="button" class="close" data-dismiss="modal" aria-label="Close">
					<span aria-hidden="true">&times;</span>
				  </button>
				</div>
				<div class="modal-body">
                    <p>Please enter the information below to add a new spring to the site.</p>
                    <p><label for="springnamem">Spring Name:</label></p>
                    <p><input type="text" id="springnamem"/></p>
                    <p><label for="descriptionm">Description of the spring:</label></p>
                    <p><input type="text" id="descriptionm"/></p>
                    <p><label for="campingm">Camping allowed?</label>
                    <input type="checkbox" id="campingm"/></p>
                    <p><label for="petfriendlym">Pets allowed?</label>
                    <input type="checkbox" id="petfriendlym"/></p>
                    <p><label for="addressm">Address of the spring:</label></p>
                    <p><input type="text" id="addressm"/></p>
				</div>
				<div class="modal-footer">
				  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
				  <button type="submit" id="newSpring" class="btn btn-primary">Submit</button>
				</div>
			  </div>
			</div>
		  </div>
