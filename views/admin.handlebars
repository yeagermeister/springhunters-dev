						<div class="form-group">
							<label for="exampleInputUsername">{{user.username}}</label>
							<input type="text" class="form-control" id="exampleInputUsername" aria-describedby="usernameHelp" placeholder="Enter username">
							<button type="submit" class="btn btn-primary mt-2">{{user.Submit}}</button>
						</div>
					</form>
					<form>
						<div class="form-group">
							<input type="text" id="id" aria-describedby="id" placeholder="id">
